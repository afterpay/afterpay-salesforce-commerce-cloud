                <description xml:lang="x-default">Country used for Afterpay payment</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="apExpressCheckout">
                <display-name xml:lang="x-default">Afterpay Express Checkout</display-name>
                <description xml:lang="x-default">Afterpay Express Checkout</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="apExpressCheckoutChecksum">
                <display-name xml:lang="x-default">Afterpay Express Checkout Capture Checksum</display-name>
                <description xml:lang="x-default">Afterpay Express Checkout Capture Checksum</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Afterpay">
                <display-name xml:lang="x-default">Afterpay</display-name>
                <attribute attribute-id="apPaymentID"/>
                <attribute attribute-id="apToken"/>
                <attribute attribute-id="apAuthoriseStatus"/>
                <attribute attribute-id="apPaymentMode"/>
                <attribute attribute-id="apDirectPaymentStatus"/>
