            <attribute-definition attribute-id="apPaymentMode">
                <display-name xml:lang="x-default">Afterpay Payment Mode</display-name>
                <description xml:lang="x-default">Afterpay Payment Mode</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="apRefundID">
                <display-name xml:lang="x-default">Afterpay Refund ID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="apRefundRequestId">
                <display-name xml:lang="x-default">Afterpay Refund Request ID</display-name>
                <description xml:lang="x-default">RequestId used for latest refund call (basically a timestamp of the call)</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
