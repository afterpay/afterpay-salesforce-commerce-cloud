<?xml version="1.0" encoding="UTF-8"?>
<preferences xmlns="http://www.demandware.com/xml/impex/preferences/2007-03-31">
    <custom-preferences>
        <all-instances/>
        <development>
            <preference preference-id="apBrandSettings">{
                "GB": {
                    "brand": "clearpay",
                    "service": "clearpay.service.GB.v2",
                },
                "AU": {
                    "brand": "afterpay",
                    "service": "afterpay.service.AU.v2",
                },
                "NZ": {
                    "brand": "afterpay",
                    "service": "afterpay.service.NZ.v2",
                },
                "CA": {
                    "brand": "afterpay",
                    "service": "afterpay.service.CA.v2",
                },
                "US": {
                    "brand": "afterpay",
                    "service": "afterpay.service.US.v2",
                }
            }</preference>
            <preference preference-id="apDisplayPdpInfo">true</preference>
            <preference preference-id="apDisplayPlpInfo">false</preference>
            <preference preference-id="apPaymentMode">DIRECT_CAPTURE</preference>
            <preference preference-id="enableAfterpay">false</preference>
            <preference preference-id="enableCashAppPay">false</preference>
            <preference preference-id="apEnableExpressCheckout">true</preference>
            <preference preference-id="apExpressCheckoutShippingStrategy">integrated</preference>
            <preference preference-id="apEnableExpressCheckoutBuyNow">true</preference>
            <preference preference-id="apStorePickupDescription">Available for next-day pickup</preference>
            <preference preference-id="apEnableExpressCheckoutPdp">false</preference>
        </development>
        <staging/>
        <production/>
    </custom-preferences>
</preferences>

