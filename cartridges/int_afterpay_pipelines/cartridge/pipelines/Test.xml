        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <decision-node condition-key="dw.system.System.instanceType != dw.system.System.PRODUCTION_SYSTEM" condition-operator="expr">
